{
  "name": "protopedia-api-v2-client",
  "version": "1.2.1",
  "description": "TypeScript client for ProtoPedia API v2",
  "engines": {
    "node": ">=20"
  },
  "private": false,
  "type": "module",
  "keywords": [
    "api",
    "client",
    "protopedia",
    "nodejs",
    "typescript"
  ],
  "author": "F88 <685250+F88@users.noreply.github.com>",
  "license": "CC0-1.0",
  "homepage": "https://github.com/F88/protopedia-api-v2-client.js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F88/protopedia-api-v2-client.js.git"
  },
  "bugs": {
    "url": "https://github.com/F88/protopedia-api-v2-client.js/issues"
  },
  "scripts": {
    "clean": "rimraf dist lib lib-types coverage docs markdown",
    "test": "vitest run tests/unit/ tests/integration",
    "test:e2e": "vitest run tests/e2e/",
    "test:coverage": "vitest run --coverage tests/unit/ tests/integration",
    "test:codecov": "vitest run --coverage --reporter=default --reporter=junit --outputFile.junit=test-report.junit.xml tests/unit/ tests/integration",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "format": "npm run prettier:write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "gen:version": "node ./scripts/generate-version.mjs",
    "prepack": "npm run gen:version",
    "prebuild": "npm run gen:version && npm run clean && npm run format && npm run lint:fix && npm run test:codecov",
    "build": "tsc --project tsconfig.build.json",
    "typecheck": "tsc --project tsconfig.dev.json --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.7.5",
    "@vitest/coverage-v8": "^4.0.15",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "msw": "^2.12.4",
    "prettier": "3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  },
  "main": "lib/src/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/src/index.js",
      "require": "./lib/src/index.js"
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "lib/src/**/*",
    "lib/types/**/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
